{
  "name": "NewTabs",
  "version": "0.0.1",
  "description": "An extention of a powerful new tab",
  "action": {
    "default_popup": "popup/index.html"
  },
  "permissions": [
    "unlimitedStorage",
    "search",
    "storage",
    "contextMenus",
    "webRequest",
    "favicon",
    "chrome://favicon/",
    "edge://favicon/",
    "https://cn.bing.com/"
  ],
  "icons": {
    "128": "logo/google.ico"
  },
  "manifest_version": 3,
  "minimum_chrome_version": "80",
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
